=
PSFTYPE = 'PGAUSS'        PSF type: (PGAUSS, GAUSS, EXTPGAUSS)
=
FWHM = 3.23e+00       Approx FWHM of objects (pixels) along major axis.
SKY = 790             Approximate mean sky value in data numbers.
=
EPERDN = 35.0         Electrons per data number (2*WFI), for avg = eperdn*n 
RDNOISE = 22.1       Readout noise in electrons (guess), for avg = rdnoise*sqrn(n)
=                     add any (subtracted) sky noise here
=
PARAMS_DEFAULT = 'param_default_c'          Default parameters file name.
PARAMS_OUT = 'param_out'              Output parameters file name.
IMAGE_IN = 'J0159.8.chip_01.fits'               Input image name.
IMAGE_OUT = 'image_out_J0159.8.chip_01.fits'    Output image name.
OBJECTS_IN = 'obj_in_J0159.8.chip_01'                       Input object list
OBJTYPE_IN = 'INTERNAL'
OBJECTS_OUT = 'obj_out_J0159.8.chip_01'         Output object list file name.
OBJTYPE_OUT = 'INTERNAL'
ERRORS_OUT = 'errors_out_J0159.8.chip_01'       Errors on fit to be output if output type is internal
SHADOWFILE_OUT = 'shad_out_J0159.8.chip_01'           Output shadow file name.
EMP_SUBRAS_OUT = 'psf_out_J0159.8.chip_01.fits'       Empirical PSF subraster.
LOGFILE = 'log.txt'                         Log file name.  TERM for screen.
LOGVERBOSITY = 100                          Verbosity of log file; (0-4).
COVARS_OUT = 'covar_out_J0159.8.chip_01'    File to contain covariance matrices
=
PSDIR = 'models/'
PSNAME_ROOT   'adata' model file names will be 'MNAME_ROOT####.fits'
CPSNAME_ROOT  'clean' model file names will be 'MNAME_ROOT####.fits'
MPSNAME_ROOT  'model' model file names will be 'MNAME_ROOT####.fits'
RPSNAME_ROOT  'resid' model file names will be 'MNAME_ROOT####.fits'
=
MAX_PERF  = 2000       Only average up to this number of stars to get shape parameters.
THRESHMIN = 20.0       Value of lowest threshold.
DOFINALFIT = 'NO'      Do a last fitting iteration after all objects found
CENTINTMAX = 42000.0   Obliterate if central intensity exceeds this.
CHI2MINBIG = 9.00 
FOOTPRINT_NOISE = 1.5  Expand stars in noise file by this amount (analytic).
RESIDNOISE = 0.3       Fraction of noise to ADD to noise file (analytic PSF).
=
AUTOSCALE = 'YES'      Auto-scaling of sizes by FWHM.
SCALEFITBOX = 5.0      Size of fit box in units of FWHM.
FITBOXMIN = 5.0        Smallest allowed fit box size.
=
EMP_STAR_X = 140.15    X position of empirical template
EMP_STAR_Y = 1191.05   Y position of empirical template
EMP_STAR_Z = 10340.0   central intensity of empirical template
THRESHEMP = 30000      fit emperical psfs below this value
=
END
